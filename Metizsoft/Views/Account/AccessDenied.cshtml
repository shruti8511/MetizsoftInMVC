@{
    Layout = null;
}

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Access Denied</title>

    <link href="~/dist/Accessdenied/flipclock.css" rel="stylesheet" />
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
    <script src="~/dist/Accessdenied/flipclock.js"></script>
    <style>
        @@import url('https://fonts.googleapis.com/css?family=Poppins');
        .clock{
            width: 300px;
            margin: 5% auto 0 auto !important;
            transform: translate(0, 0);
            z-index:99999;
        }
        .message { text-align: center; display: none; }
        .redirect_message{
            position: absolute;
            z-index: 999;
            width: 100%;
            text-align: center;
            margin-top: 0;
            font-family: 'Poppins', sans-serif;
            font-size: 15px;
            letter-spacing: 0.8px;
            color: #333333;
            top: 2%;
        }

    </style>
</head>




<body style="overflow: hidden; margin: 0 auto;">
    <h6 class="redirect_message">You will automatically be redirect to homepage in...</h6>
    <div class="clock" style="margin:2em;"></div>
    <div class="message"></div>


    <a href="@Url.Action("Index","Home")" target="_self">
        @*<img src="~/img/Access_9.jpg" style="width:100%;position: absolute;top: 0;" />*@
        <img src="~/dist/Accessdenied/Access_9.jpg" style="width:100%;position: absolute;top: 0;" />
    </a>

    <script type="text/javascript">
    var clock;
    $(document).ready(function () {
        clock = $('.clock').FlipClock(05, {
            clockFace: 'MinuteCounter',
            countdown: true,
            callbacks: {
                stop: function () {
                    setTimeout(function () {
                        window.location.href = '@Url.Action("Index","Home")';
                    }, 1000);
                }
            }
        });

    });
    </script>
</body>
</html>
